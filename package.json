{
  "name": "monitor",
  "version": "1.0.0",
  "productName": "monitor",
  "description": "Monitor of spring boot",
  "author": "gosuncn",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js --inline --hot --quiet --port 3000",
    "dev": "cross-env NODE_ENV=development electron ./",
    "build:watch": "cross-env NODE_ENV=development webpack --config build/webpack.dev.js  --profile --colors --watch",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "lint": "eslint src/* --ext .js --ext .vue",
    "package": "npm run build; npm run package-osx; npm run package-win; npm run package-linux;",
    "package-osx": "electron-packager ./ --platform=darwin --arch=x64 --icon=assets/app.icns --out=dist/bin --asar=true --overwrite=force",
    "package-win": "electron-packager ./ --platform=win32 --arch=ia32 --icon=src/assets/app.ico --out=dist/bin --asar=true --overwrite=force",
    "package-win64": "electron-packager ./ --platform=win32 --arch=x64 --icon=src/assets/app.ico --out=dist/bin --asar=true --overwrite=force",
    "package-linux": "electron-packager ./ --platform=linux --arch=ia32 --icon=src/assets/app.ico --out=dist/bin --asar=true --overwrite=force"
  },
  "dependencies": {
    "electron": "1.4.12",
    "lodash": "^4.17.3",
    "moment": "^2.17.1",
    "muse-ui": "^2.0.0-beta.1",
    "vue": "^2.0.1",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-component": "^0.6.0",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-latest": "*",
    "babel-preset-stage-1": "^6.13.0",
    "babel-runtime": "^5.8.0",
    "cross-env": "^3.1.1",
    "css-loader": "^0.25.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.4.0",
    "electron-reload": "1.0.3",
    "element-ui": "^1.0.6",
    "eslint": "^3.6.0",
    "eslint-config-vue": "^1.0.0",
    "eslint-plugin-html": "latest",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "keycode": "^2.1.7",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.0.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
